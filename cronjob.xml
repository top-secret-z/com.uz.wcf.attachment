<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/cronjob.xsd">
    <import>
        <cronjob name="com.uz.wcf.attachment.cleanup">
            <classname>wcf\system\cronjob\AttachmentLogCleanupCronjob</classname>
            <description>Delete outdated attachment log entries</description>
            <description language="de">Löscht veraltete Dateianhang-Protokolleinträge</description>
            <startminute>30</startminute>
            <starthour>1</starthour>
            <startdom>*</startdom>
            <startmonth>*</startmonth>
            <startdow>*</startdow>
            <canbeedited>1</canbeedited>
            <canbedisabled>1</canbedisabled>
        </cronjob>
    </import>
</data>
